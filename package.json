    "test": "pnpm test:unit && pnpm test:e2e",
    "check:favicon": "realfavicon check -s cli 3000",
    "scan:secrets": "ggshield secret scan repo .",
    "verify": "pnpm lint && pnpm test && pnpm scan:secrets",
    "predev": "pnpm lint:fix",
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start"
  },
