  "scripts": {
    "prepare": "husky || true",
    "lint": "biome ci .",
    "lint:fix": "biome check --write .",
    "test:unit": "vitest",
    "test:unit:update-snapshots": "vitest --update",
    "test:e2e": "playwright test",
